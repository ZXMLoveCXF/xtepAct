/* pages/activity/result/result.wxss */
.head{margin:30rpx;border:1px solid #e0e0e0;border-radius:10rpx;width:690rpx;color:#000000;padding-bottom:30rpx;-moz-box-shadow:0px 2px 9px #CCCCCC; -webkit-box-shadow:0px 2px 9px #CCCCCC; box-shadow:0px 2px 9px #CCCCCC;}
.head.win{background-image:url(http://xtep321go-live.image.alimmdn.com/image/system/users/admin/20180116150009n9tNwZOd.png);background-position:center;background-repeat:no-repeat;background-size:100% auto;}
.head .pri-img{height:200rpx;width:200rpx;margin-left:245rpx;margin-top:80rpx;}
.head .no-win-img{height:160rpx;width:320rpx;margin-left:185rpx;margin-top:100rpx;}
.head .no-join-img{height:160rpx;width:160rpx;margin-left:265rpx;margin-top:100rpx;}
.head .pri-title{font-size:38rpx;color:#353535;text-align:center;margin:20rpx 0;font-weight:600}
.head .pri-title.win{color:#e54341;}
.head .pri-content{color:#353535;text-align:center;font-size:30rpx;font-weight:600}
.head .btn{border-radius:50rpx;margin:30rpx 150rpx;text-align:center;}
.head .btn.blu{background-color:#4fb0f4;color:#fff;font-size:30rpx;}
.head .btn.whi{border:1px solid #4fb0f4;color:#4fb0f4;font-size:30rpx;}
.head .detail{text-align:center;margin-top:20rpx;color:#989898;font-size:28rpx;display:flex;justify-content:center;}
.head .detail .right-img{width:13rpx;height:18rpx;margin-left:10rpx;}
.head .detail .right-img image{width:13rpx;height:18rpx;opacity:0.6;}
.head .win-title{color:#f64849;text-align:center;margin-top:40rpx;font-weight:600;font-size:32rpx;}
.head .result{text-align:center;margin-top:10rpx;font-size:28rpx;font-weight:600;}
.head .result-c{text-align:center;font-weight:28rpx;margin-bottom:10rpx;}
.head .result text{font-size:45rpx;}
.head .win-img{height:180rpx;width:180rpx;margin-left:255rpx;margin-top:20rpx;} 
.head .btn-group{display:flex;justify-content:center;margin-top:78rpx;margin-bottom:30rpx;}
.head .btn-group .btn{width:250rpx;font-size:30rpx;padding:20rpx;border-radius:100rpx;margin:-45rpx 10rpx 0 10rpx;}
.head .btn-group .btn.l{background-color:#4db0f4;color:#fff;}
.head .btn-group .btn.r{background-color:#fff;border:1px solid #4db0f4;color:#4db0f4;}
.title{color:#353535;text-align:center;font-size:35rpx;display:flex;margin:90rpx auto 20rpx auto;font-weight:600;justify-content:center;}
.title view{background-color:#353535;height:5rpx;width:25rpx;border-radius:8rpx;margin:22rpx 10rpx 0 10rpx;}
.user-list{padding:30rpx;}
.user-list .ali-box{margin:0 auto;}
.user-list .ali-box .user{width:100rpx;float:left;margin:10rpx 19rpx;}
.user-list .ali-box .user image{width:100rpx;height:100rpx;border-radius:100rpx;border:1px solid #e0e0e0;}
.content{width:550rpx;margin:100rpx;text-align:center;}
.content image{width:117rpx;height:90rpx;padding-bottom:20rpx;}
.content text{color:#c2c2c2;font-size:30rpx;}

button{margin:0;padding:0;background-color:#fff;}
/*mask*/  
.drawer_screen{width:100%;height:100%;position:fixed;top:0;left:0;z-index:1000;background:#000;opacity:0.2;overflow:hidden;}  
/*content*/  
.drawer_attr_box{width:100%;overflow:hidden;position:fixed;bottom:0;left:0;z-index:1001;background:#fff;}  
.drawer_content{padding:20rpx 0;height:280rpx;overflow-y:hidden;}  
.drawer_title{padding:20rpx;font:35rpx "microsoft yahei";text-align:center;}  
.line{border-bottom:5px solid #f8f8f8;}  


.drawer_attr_box1{width:650rpx;overflow:hidden;position:fixed;left:0;z-index:1001;background:#fff;margin:0 50rpx;border-radius:10rpx;} 
.drawer_content1{padding-top:20rpx;overflow-y:hidden;}
.drawer_content1 .tip{text-align:center;font-weight:600;margin-top:20rpx;}
.drawer_content1 .msg{text-align:center; margin-top:20rpx;font-size:30rpx;color:#929292;}
.drawer_content1 .btn-group{display:flex;justify-content:space-around;}
.drawer_content1 .btn-group .one{font-size:35rpx;width:50%;text-align:center;padding:20rpx;}
.drawer_content1 .btn-group .one.l{border-top:1px solid #f1f1f1;border-right:1px solid #f1f1f1;}
.drawer_content1 .btn-group .one.r{border-top:1px solid #f1f1f1;border-left:1px solid #f1f1f1;color:#6ac341;}
.drawer_content1 .sure{padding:0 50rpx;margin:20rpx 0;}
.drawer_content1 .sure checkbox{color:#6ac341;zoom:60%;}